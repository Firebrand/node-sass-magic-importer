{
  "name": "node-sass-magic-importer",
  "description": "Custom node-sass importer for selector specific imports, node importing, module importing, globbing support and importing files only once",
  "keywords": [
    "glob-importer",
    "importer",
    "module-importer",
    "node-sass",
    "package-importer",
    "sass",
    "selector-importer"
  ],
  "version": "4.1.4",
  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/node-sass-magic-importer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maoberlehner/node-sass-magic-importer"
  },
  "bugs": {
    "url": "https://github.com/maoberlehner/node-sass-magic-importer/issues"
  },
  "dependencies": {
    "css-node-extract": "^1.0.0",
    "css-selector-extract": "^3.0.1",
    "postcss-scss": "^1.0.2"
  },
  "devDependencies": {
    "@types/glob": "^5.0.30",
    "@types/node": "^8.0.5",
    "@types/sinon": "^2.3.2",
    "ava": "^0.20.0",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.5",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run clean && tsc -w",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "tslint 'src/**/*.ts'",
    "test": "npm run build && ava dist/**/*.spec.js test/**/*.test.js",
    "coverage": "npm run build && nyc ava dist/**/*.spec.js"
  }
}
