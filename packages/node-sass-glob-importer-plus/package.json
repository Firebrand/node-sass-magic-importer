{
  "name": "node-sass-glob-importer-plus",
  "description": "Like node-sass-glob-importer except also parses vaguer paths without the .scss or .sass extension (like vendors/**/*)",
  "keywords": [
    "glob-importer",
    "importer",
    "node-sass",
    "sass"
  ],
  "version": "5.3.2",
  "author": "Markus Oberlehner",
  "contributors": ["Alex Tsirozidis"],
  "homepage": "https://github.com/Firebrand/node-sass-magic-importer",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc -w",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Firebrand/node-sass-magic-importer"
  },
  "bugs": {
    "url": "https://github.com/Firebrand/node-sass-magic-importer/issues"
  },
  "dependencies": {
    "node-sass-magic-importer": "^5.3.2"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.11.1",
    "npm": ">=3.0.0"
  }
}
